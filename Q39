//Q39: Write a program to find the product of odd digits of a number.

/*
Sample Test Cases:
Input 1:
12345
Output 1:
15 (1*3*5)

Input 2:
2468
Output 2:
1 (no odd digits, assume 1)

*/
#include <stdio.h>

int main() 
{
    int num, digit;
    long long product = 1; // Initialize product to 1

   
    printf("Input an integer: ");
    
    
    scanf("%d", &num);

   
    if (num % 10 % 2 != 0 && num / 10 == 0) {
        product = num;
    }
else 
{
        
        while (num != 0) {
            digit = num % 10; 
            if (digit % 2 != 0) 
{
                product *= digit; 
            }
            
            num /= 10; 
        }
    }

    printf("%lld\n", product);

    return 0;
}
