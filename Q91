//Q91: Remove all vowels from a string.

/*
Sample Test Cases:
Input 1:
education
Output 1:
dctn

*/
#include <stdio.h>

int main() {
    char str[1000], result[1000];
    int j = 0;

    // Read input string
    fgets(str, sizeof(str), stdin);

    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];

        // Convert to lowercase for uniform comparison
        if (ch >= 'A' && ch <= 'Z') {
            ch = ch + ('a' - 'A');
        }

        // Check if character is not a vowel
        if (ch != 'a' && ch != 'e' && ch != 'i' && ch != 'o' && ch != 'u' && str[i] != '\n') {
            result[j++] = str[i];
        }
    }

    result[j] = '\0'; // Null-terminate the result string
    printf("%s\n", result);

    return 0;
}
